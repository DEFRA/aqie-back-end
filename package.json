{
  "name": "aqie-back-end",
  "version": "0.1.0",
  "description": "CDP Backend Template",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.16.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm run server:watch",
    "dev:debug": "NODE_ENV=development npm run server:debug",
    "docker:dev": "npm run dev",
    "format": "prettier --write \"**/*.{cjs,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md}\"",
    "git:pre-commit-hook": "npm run format:check && npm run lint",
    "lint": "eslint --cache --cache-strategy content \"**/*.{cjs,js}\"",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm run setup:husky",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,json --legacy-watch ./src",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,json --legacy-watch ./src",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "start": "NODE_ENV=production node --use-strict .",
    "test": "TZ=UTC vitest run --coverage",
    "test:watch": "TZ=UTC vitest"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@defra/hapi-tracing": "1.22.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/cookie": "12.0.1",
    "@hapi/hapi": "21.4.0",
    "@hapi/yar": "11.0.3",
    "await-reduce": "1.2.3",
    "awaity": "1.0.0",
    "aws-embedded-metrics": "4.2.0",
    "aws4": "1.13.2",
    "global-agent": "3.0.0",
    "convict-format-with-validator": "6.2.0",
    "babel-plugin-module-resolver": "5.0.0",
    "buffer": "6.0.3",
    "hapi-pulse": "3.0.1",
    "convict": "6.2.4",
    "dayjs": "1.11.13",
    "fast-xml-parser": "4.5.0",
    "glob": "11.0.3",
    "hapi-pino": "12.1.0",
    "https-proxy-agent": "7.0.6",
    "joi": "17.13.3",
    "mongo-locks": "3.0.2",
    "lodash": "4.17.21",
    "lru-cache": "11.1.0",
    "moment": "2.30.1",
    "moment-timezone": "0.5.45",
    "mongodb": "6.17.0",
    "node-cron": "3.0.3",
    "node-fetch": "3.3.2",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "rimraf": "6.0.1",
    "save-dev": "0.0.1-security",
    "ssh2": "1.16.0",
    "ssh2-sftp-client": "12.0.0",
    "tunnel": "0.0.6",
    "undici": "7.10.0",
    "url": "0.11.4",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@babel/preset-env": "7.27.2",
    "eslint": "9.29.0",
    "husky": "9.1.7",
    "neostandard": "0.12.1",
    "nodemon": "3.1.10",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "resolve-url-loader": "5.0.0",
    "vitest": "3.2.4",
    "vitest-fetch-mock": "0.4.5",
    "vitest-mongodb": "1.0.1",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "10.4.21"
  }
}
